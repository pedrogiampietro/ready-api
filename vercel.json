{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "node_modules/.prisma/**"
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_PRISMA_GENERATE": "true"
    }
  }
}
